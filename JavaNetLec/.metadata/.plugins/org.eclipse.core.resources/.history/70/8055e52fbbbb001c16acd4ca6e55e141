package thread.ex07.notsyncthread;

class Sum implements Runnable{
	
	static int num = 0;
	int start, end;
	
	Sum(int start, int end){
		this.start = start;
		this.end = end;
	}
	
	void accumulate(int val) {
		num += val;
	}

	@Override
	public void run() {
		for(int i=start;i<=end;i++)
			accumulate(i);
	}	
}

public class NotSyncThread {
	public static void main(String[] args) {
		
	}
}
